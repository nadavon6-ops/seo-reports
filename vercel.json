{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/cardioteam",
      "destination": "/cardioteam_2025_annual_report.html"
    },
    {
      "source": "/manpower",
      "destination": "/manpower_2025_comprehensive.html"
    },
    {
      "source": "/maromg",
      "destination": "/maromg_2025_comprehensive.html"
    }
  ]
}
